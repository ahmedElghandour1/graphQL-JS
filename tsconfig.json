{
    "compilerOptions": {
      "outDir": "./dist/",
      "target": "esnext",
      "module": "esnext",
      "strict": false,
      "noImplicitAny": false,
      "jsx": "preserve",
      "importHelpers": true,
      "moduleResolution": "node",
      "allowJs": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "sourceMap": true,
      "experimentalDecorators": true,
      "baseUrl": ".",
      "paths": {
        "@/*": [
          "src/*"
        ]
      }
    },
    "exclude": [
      "node_modules"
    ]
  }
  